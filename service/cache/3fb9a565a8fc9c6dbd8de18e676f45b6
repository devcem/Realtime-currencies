    $('#guidelinesCheckbox'+66).on('change', function(){
        if ($('#guidelinesCheckbox'+66).attr('checked') == 'checked')
            $('#guidelinesButton'+66).removeClass('disabled');
        else
            $('#guidelinesButton'+66).addClass('disabled');
    });

    $('a[id^="guidelinesButton"]').on('click', function(){
        if ($(this).hasClass('disabled')) {
            return false;
        }
    });
</script>
