    $('#guidelinesCheckbox'+18).on('change', function(){
        if ($('#guidelinesCheckbox'+18).attr('checked') == 'checked')
            $('#guidelinesButton'+18).removeClass('disabled');
        else
            $('#guidelinesButton'+18).addClass('disabled');
    });

    $('a[id^="guidelinesButton"]').on('click', function(){
        if ($(this).hasClass('disabled')) {
            return false;
        }
    });
</script>
